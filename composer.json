{
    "name": "webmacth/wbm-review-vote-plugin",
    "description": "Review Vote Plugin",
    "type": "shopware-platform-plugin",
    "license": "MIT",
    "extra": {
        "shopware-plugin-class": "Wbm\\WbmReviewVotePlugin",
        "author": "Ron Gruse",
        "label": {
            "de-DE": "Abstimmen für Bewertungen für Shopware 6",
            "en-GB": "Vote for Reviews for Shopware 6"
        }
    },
    "require": {
        "php": ">= 7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~8.3.4",
        "phpstan/phpstan": "^0.12.18",
        "shopware/core": "*",
        "shopware/storefront": "*"
    },
    "autoload": {
        "psr-4": {
            "Wbm\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wbm\\Tests\\": "tests/"
        }
    }
}
